<%= form_for @director do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :required => true %>
    <%= f.label :photo %>
    <%= f.url_field :photo %>
    <%= f.label :birthplace %>
    <%= f.text_field :birthplace %>
    <%= f.label :dob, 'Date of birth' %>
    <%= f.text_field :dob, :placeholder => 'YYYY-MM-DD' %>
    <%= f.submit %>
<% end %>